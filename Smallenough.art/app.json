    "ios": {
      "bundleIdentifier": "smallenough.art",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "smallenough.art",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }

}
